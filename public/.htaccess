<IfModule mod_rewrite.c>

    RewriteEngine On
    RewriteBase /proyectos/extraccion-inmediata/
    RewriteRule ^/proyectos/extraccion-inmediata/index\.html$ - [L]
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d
    RewriteRule . /proyectos/extraccion-inmediata/index.html [L]
</IfModule>
